{
  "name": "shoppinglist",
  "version": "1.0.0",
  "description": "shopping list \u001b[D\u001b[D\u001b[D\u001b[D\u001b[\u001b[C\u001b[C\u001b[\u001b[C",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . snap-atac --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Harry",
  "license": "ISC",
  "dependencies": {
    "electron": "^7.1.2",
    "electron-pdf-window": "^1.0.12",
    "r-script": "0.0.4"
  },
  "devDependencies": {
    "electron-package": "^0.1.0",
    "electron-packager": "^14.1.1"
  }
}
